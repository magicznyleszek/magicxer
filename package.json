{
  "name": "sicx",
  "version": "0.0.1",
  "description": "Sick word mixer",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watchAll --coverage",
    "start": "parcel -p 2038 src/index.html -d dev",
    "build": "parcel build src/index.html -d docs --public-url ./ --no-cache && npm run copy-static-files",
    "copy-static-files": "cp CNAME favicon.png humans.txt LICENSE manifest.json favicon.ico favicon.png docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smutnyleszek/sicx.git"
  },
  "author": "Leszek Pietrzak",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/smutnyleszek/sicx/issues"
  },
  "homepage": "https://github.com/smutnyleszek/sicx#readme",
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "@types/lodash": "^4.14.101",
    "autoprefixer": "^7.2.6",
    "jest": "^22.2.0",
    "lodash": "^4.17.5",
    "parcel-bundler": "^1.4.1",
    "parcel-plugin-vue": "^1.5.0",
    "postcss": "^6.0.19",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "stylelint": "^8.4.0",
    "stylelint-order": "^0.8.0",
    "ts-jest": "^22.0.3",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "vue": "^2.5.13",
    "vuex": "^3.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
