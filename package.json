{
  "name": "sicx",
  "version": "0.0.1",
  "description": "Sick word mixer",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watchAll",
    "start": "npm run clear && parcel -p 2038 src/index.html",
    "build": "npm run clear && parcel build src/index.html",
    "clear": "rm -rf dist && rm -rf .cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smutnyleszek/sicx.git"
  },
  "author": "Leszek Pietrzak",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/smutnyleszek/sicx/issues"
  },
  "homepage": "https://github.com/smutnyleszek/sicx#readme",
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "@types/lodash": "^4.14.101",
    "autoprefixer": "^7.2.5",
    "jest": "^22.2.0",
    "lodash": "^4.17.4",
    "parcel-bundler": "^1.4.1",
    "parcel-plugin-vue": "^1.5.0",
    "postcss-cssnext": "^3.1.0",
    "ts-jest": "^22.0.2",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "vue": "^2.5.13",
    "vuex": "^3.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  }
}
